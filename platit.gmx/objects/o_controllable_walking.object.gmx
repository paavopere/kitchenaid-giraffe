            <string>/// Basic movement

var jumped;
jumped = 0;

// gravity
if place_free(x, y + 1) {
    gravity = 1;
    gravity_direction = 270;
} else {
    gravity = 0;
    gravity_direction = 270;
    move_snap(0, 1);
    self.double_jumped = false;
}

// keys
if keyboard_check(self.up_key) and (o_ctrl_gameplay.active_controllable == self.id
                                    or self.always_controllable) {
    // jumped: if they jumped during this very step
    jumped = jump(self.jump_speed);
}
if keyboard_check(self.left_key) and (o_ctrl_gameplay.active_controllable == self.id 
                                      or self.always_controllable) {
    walk_left(self.walk_speed);
}
if keyboard_check(self.right_key) and (o_ctrl_gameplay.active_controllable == self.id
                                       or self.always_controllable) {
    walk_right(self.walk_speed);
}
if self.double_jump_active 
    and keyboard_check_pressed(self.up_key) 
    and not jumped 
    and not self.double_jumped 
    and (o_ctrl_gameplay.active_controllable == self.id
         or self.always_controllable) {
    // double jump action
    event_user(0);
}
</string>
